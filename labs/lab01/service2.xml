        <query>var dbConn;

try {
	dbConn = DatabaseConnectionFactory.createDatabaseConnection(&apos;com.mysql.cj.jdbc.Driver&apos;,&apos;jdbc:mysql://localhost:3306/medical_work_list&apos;,&apos;service2&apos;,&apos;svc2&apos;);
	if ($(&apos;orcAction&apos;) == &quot;CA&quot;) {
		cancelRequest($(&apos;requestID&apos;))
	} else if ($(&apos;orcAction&apos;) == &quot;NW&quot;) {
		insertRequest($(&apos;requestID&apos;))
	}
} finally {
	if (dbConn) { 
		dbConn.close();
	}
}


function cancelRequest(reqId) {
	var vSQL = &quot;UPDATE work_list SET status=&apos;canceled&apos; WHERE request_id=&quot;+reqId;
    	result = dbConn.executeUpdate(vSQL);
	logger.info(result);
}


function insertRequest(reqId) {
	var vSQL = &quot;INSERT INTO work_list (request_id, date, hour, patient_id, patient_name, patient_address, patient_phone_number, episode_number, info) VALUES (&quot;;
	vSQL = vSQL + reqId + &quot;, &apos;&quot; + $(&apos;requestDate&apos;) + &quot;&apos;, &apos;&quot; + $(&apos;requestTime&apos;) + &quot;&apos;, &quot; + $(&apos;patientID&apos;) + &quot;, &apos;&quot; + $(&apos;patientName&apos;) + &quot;&apos;, &apos;&quot; + $(&apos;patientAddr&apos;) + &quot;&apos;, &apos;&quot;;
	vSQL = vSQL + $(&apos;patientPhoneNumber&apos;) + &quot;&apos;, &quot; + $(&apos;episodeNumber&apos;) + &quot;, &apos;&quot; + $(&apos;clinicalInfo&apos;) + &quot;&apos;)&quot;;
    	result = dbConn.executeUpdate(vSQL);
	logger.info(result);
}
</query>
